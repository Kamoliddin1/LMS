<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.book.BookController">
    <children>
        <TableView fx:id="TableBook">
            <columns>
                <TableColumn fx:id="cId" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="cTitle" prefWidth="75.0" text="TITLE" />
                <TableColumn fx:id="cSubject" prefWidth="75.0" text="SUBJECT" />
                <TableColumn fx:id="cAuthor" prefWidth="75.0" text="AUTHOR" />
                <TableColumn fx:id="cISBN" prefWidth="75.0" text="ISBN" />
<!--                <TableColumn fx:id="cBorDate" prefWidth="75.0" text="B DATE" />-->
<!--                <TableColumn fx:id="cDedDate" prefWidth="75.0" text="D DATE" />-->
<!--                <TableColumn fx:id="cStatus" prefWidth="75.0" text="STATUS" />-->
            </columns>
        </TableView>
    </children>
    <Button layoutX="524.0" layoutY="315.0" mnemonicParsing="false" onAction="#handleBookDeleteCheckbox" text="Delete" />
    <Button layoutX="526.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleBookCreateCheckbox" text="Create" />
</AnchorPane>
