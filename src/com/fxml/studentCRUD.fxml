<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<AnchorPane prefHeight="385.0" prefWidth="526.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.StudentCRUDController">
    <children>
        <TableView fx:id="TableStudent" prefHeight="385.0" prefWidth="526.0">
            <columns>
                <TableColumn fx:id="cID" prefWidth="60.0" text="ID" />
                <TableColumn fx:id="cUniID" prefWidth="100.0" text="UNI ID" />
                <TableColumn fx:id="cFirstName" prefWidth="110.0" text="FIRST NAME" />
                <TableColumn fx:id="cLastName" prefWidth="110.0" text="LAST NAME" />
