            </columns>
        </TableView>
    </children>
    <Button layoutX="449.0" layoutY="291.0" mnemonicParsing="false" onAction="#handleStudentDeleteCheckbox" text="Delete" />
    <Button layoutX="448.0" layoutY="359.0" mnemonicParsing="false" onAction="#handleStudentCreateCheckbox" text="Create" />
    <Button layoutX="446.0" layoutY="326.0" mnemonicParsing="false" onAction="#handleStudentUpdateCheckbox" text="Update" />
    <Button layoutX="445.0" layoutY="256.0" mnemonicParsing="false" onAction="#refreshTable" text="Refresh" />
</AnchorPane>
