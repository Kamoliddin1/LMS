<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.book.BookController">
    <children>
        <TableView fx:id="TableBook">
            <columns>
                <TableColumn fx:id="cId" prefWidth="88.0" text="ID" />
                <TableColumn fx:id="cTitle" prefWidth="79.0" text="TITLE" />
                <TableColumn fx:id="cSubject" prefWidth="85.0" text="SUBJECT" />
                <TableColumn fx:id="cAuthor" prefWidth="72.0" text="AUTHOR" />
                <TableColumn fx:id="cISBN" prefWidth="63.0" text="ISBN" />
                <TableColumn fx:id="cPublish_Date" prefWidth="75.0" text="PUBLISHED" />
                <TableColumn fx:id="cBookCRUD" prefWidth="75.0" text="Checkbox" />
            </columns>
        </TableView>
    </children>
    <Button layoutX="450.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleBookDeleteCheckbox" text="Delete" />
    <Button layoutX="450.0" layoutY="310.0" mnemonicParsing="false" onAction="#handleBookCreateCheckbox" text="Create" />
    <Button layoutX="450.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleBookUpdateCheckbox" text="Update" />
    <Button layoutX="450.0" layoutY="250.0" mnemonicParsing="false" onAction="#refreshTable" text="Refresh" />
</AnchorPane>
